{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": [
            "last 5 Chrome versions",
            "last 5 Firefox versions",
            "last 3 Safari versions",
            "last 3 iOS versions",
            "last 3 Android version",
            "last 3 ChromeAndroid version",
            "ie 11"
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "transform-react-jsx", {
        "pragma": "wp.element.createElement"
      }
    ]
  ]
}